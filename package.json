{
  "name": "fp-ts-iterable",
  "main": "dist/cjs/index.js",
  "module": "dist/cjs/index.js",
  "types": "dist/types/index.d.ts",
  "homepage": "https://github.com/waynevanson/fp-ts-iterable",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && run-p build:*",
    "build:cjs": "tsc --outDir dist/cjs --target es5 --module commonjs ",
    "build:esm": "tsc --outDir dist/esm --target es6 --module es6",
    "build:types": "tsc --outDir dist/types --declaration --emitDeclarationOnly",
    "prepare": "husky install"
  },
  "dependencies": {
    "fp-ts": "^2.12.1"
  },
  "devDependencies": {
    "@jest/types": "^28.1.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.39",
    "docs-ts": "^0.6.10",
    "fast-check": "^2.6.0",
    "fp-ts-laws": "^0.3.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^13.0.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.1.8",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^28.0.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "lint-staged": {
    "*.ts": "jest -o --passWithNoTests",
    "*!(.spec).ts": "docs-ts"
  },
  "husky": {},
  "version": "4.2.2"
}
